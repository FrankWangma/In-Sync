{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend server for the in-sync SOFTENG 750 project at the uoa",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-modules --experimental-json-modules server.js",
    "start-dev": "nodemon --exec babel-node --presets @babel/preset-env server.js",
    "test": "jest",
    "release": "npm run clean && npm run build && npm run serve",
    "clean": "rm -rf dist && mkdir dist",
    "build": "babel . -s -D -d dist --presets @babel/preset-env --ignore node_modules",
    "serve": "node dist/server.js",
    "lint": "eslint src/**/*.js",
    "fix-lint": "eslint src/**/*.js --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@std/esm": "^0.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.7",
    "mongodb-memory-server-core": "^6.6.1",
    "mongoose": "^5.9.7",
    "nanoid": "^3.1.9",
    "rootpath": "^0.1.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.9.6",
    "@types/jest": "^25.2.3",
    "axios": "^0.19.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^26.0.1",
    "mongodb-memory-server": "^6.6.1",
    "nodemon": "^2.0.2",
    "regenerator-runtime": "^0.13.5"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "engines": {
    "node": "14.3.0"
  }
}
